<Project>
  <PropertyGroup>
    <AnalysisLevel>latest</AnalysisLevel>
    <AnalysisMode>All</AnalysisMode>
    <AssemblyOriginatorKeyFile>$(RepoRoot)assets/instantia.snk</AssemblyOriginatorKeyFile>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
    <LangVersion>latest</LangVersion>
    <NeutralLanguage>en-US</NeutralLanguage>
    <RepoRoot>$(MSBuildThisFileDirectory)</RepoRoot>
    <RootNamespace>Instantia</RootNamespace>
  </PropertyGroup>
  <PropertyGroup>
    <Authors>Viktor T.</Authors>
    <PackageIcon>kopimi.png</PackageIcon>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/qbit86/instantia</RepositoryUrl>
  </PropertyGroup>
  <ItemGroup>
    <None Include="$(RepoRoot)assets/kopimi.png" Pack="true" PackagePath="" />
  </ItemGroup>
</Project>
